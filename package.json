    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "async": "^3.2.4",
    "browserstack-local": "^1.5.1",
    "esbuild": "^0.14.31",
    "eslint": "^8.12.0",
    "microbundle": "^0.15.0",
    "node-fetch": "^3.2.9",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
