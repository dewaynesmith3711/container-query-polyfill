  "type": "module",
  "main": "dist/src/cqfill.js",
  "module": "dist/src/cqfill.js",
  "publishConfig": {
    "source": "src/engine.ts"
  },
  "exports": {
    ".": {
      "browser": "./dist/src/cqfill.modern.js"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/src/*",
    "package.json"
